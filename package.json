{
    "name": "open-in-marked",
    "displayName": "open in marked",
    "description": "Open Markdown files in Marked2",
    "version": "0.0.1",
    "publisher": "fmoronzirfas",
    "engines": {
        "vscode": "^1.22.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.openInMarked"
    ],
    "main": "./extension",
    "contributes": {
        "configuration": {
            "title": "Open in Marked 2 Configuration",
            "type": "object",
            "properties": {
                "openinmarked.apppath": {
                    "type": "string",
                    "default": "/Applications/Marked 2.app",
                    "description": "The path to the Marked 2 application"
                }
            }
        },
        "commands": [
            {
                "command": "extension.openInMarked",
                "title": "Open In Marked 2"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.11.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}